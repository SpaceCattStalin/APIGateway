{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "chatbotRoute": {
        "ClusterId": "chatbot",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "knowledgebaseRoute": {
        "ClusterId": "knowledgebase",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "userserviceRoute": {
        "ClusterId": "userservice",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "chatbot": {
        "Destinations": {
          "chatbotService": {
            "Address": "https://localhost:7213;http://localhost:5012"
          }
        }
      },
      "knowledgebase": {
        "Destinations": {
          "knowledgebaseService": {
            "Address": "https://localhost:7212;http://localhost:5011"
          }
        }
      },
      "userservice": {
        "Destinations": {
          "userservice": {
            "Address": "https://localhost:7214;http://localhost:5013"
          }
        }
      }
    }
  }
}