{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "authserviceRoute": {
        "ClusterId": "userservice",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "userserviceRoute": {
        "ClusterId": "userservice",
        "Match": {
          "Path": "/user/{**catch-all}"
        }
      },
      "leadserviceRoute": {
        "ClusterId": "leadservice",
        "Match": {
          "Path": "/bookings/{**catch-all}"
        }
      },
      "collegeinfoserviceRoute": {
        "ClusterId": "collegeinfo",
        "Match": {
          "Path": "/collegeinfo/{**catch-all}"
        }
      },
      "swagger_user": {
        "ClusterId": "userservice",
        "Match": {
          "Path": "/swagger/user/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/{**catch-all}"
          }
        ]
      },
      "swagger_lead": {
        "ClusterId": "leadservice",
        "Match": {
          "Path": "/swagger/lead/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/{**catch-all}"
          }
        ]
      },
      "swagger_college": {
        "ClusterId": "collegeinfo",
        "Match": {
          "Path": "/swagger/collegeinfo/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "userservice": {
        "Destinations": {
          "userservice": {
            "Address": "http://user:80"
          }
        }
      },
      "collegeinfo": {
        "Destinations": {
          "collegeinfoservice": {
            "Address": "http://college:80"
          }
        }
      },
      "leadservice": {
        "Destinations": {
          "leadservice": {
            "Address": "http://lead:80"
          }
        }
      }
    }
  }
}
